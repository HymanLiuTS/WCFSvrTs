<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="MEXGET">
                    <serviceMetadata httpGetEnabled="true" httpGetUrl="MyMEXAddress"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
      <protocolMapping>
        <add scheme="http" binding="wsHttpBinding"/>
      </protocolMapping>
    </system.serviceModel>
</configuration>
